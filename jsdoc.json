{
   "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
         "jsdoc",
         "closure"
      ]
   },
   "recurseDepth": 10,
   "source": {
      "include": [
         "src/renderer"
      ],
      "includePattern": ".ts",
      "excludePattern": ".styl"
   },
   "plugins": [
      "node_modules/better-docs/typescript",
      "node_modules/better-docs/category",
      "plugins/markdown"
   ],
   "typescript": {
      "moduleRoot": "test"
   },
   "templates": {
      "better-docs": {
         "name": "Documentation"
      }
   },
   "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
   },
   "markdown": {
      "idInHeadings": true
   }
}
