{
   "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
         "jsdoc",
         "closure"
      ]
   },
   "source": {
      "include": [
         "src"
      ],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
   },
   "plugins": [
      "node_modules/better-docs/typescript",
      "node_modules/better-docs/category",
      "node_modules/better-docs/component"
   ],
   "typescript": {
      "moduleRoot": "test"
   },
   "templates": {
      "better-docs": {
         "name": "Documentation"
      }
   },
   "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
   }
}
