{
   "extends": "tslint:recommended",
   "rules": {
      "no-any": false,
      "member-access": true,
      "member-ordering": [
         true,
         {
            "order": [
               "static-field",
               "static-method"
            ]
         }
      ],
      "no-internal-module": true,
      "no-reference": false,
      "no-namespace": false,
      "no-empty-interface": true,
      "only-arrow-functions": [
         true,
         "allow-declarations",
         "allow-named-functions"
      ],
      "prefer-for-of": true,
      "curly": true,
      "forin": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-construct": true,
      "no-duplicate-super": true,
      "no-duplicate-switch-case": true,
      "no-empty": true,
      "no-eval": true,
      "no-implicit-dependencies": [
         true,
         "dev"
      ],
      "no-invalid-template-strings": true,
      "no-invalid-this": true,
      "no-shadowed-variable": true,
      "no-sparse-arrays": true,
      "no-string-literal": true,
      "no-this-assignment": true,
      "no-unused-expression": true,
      "no-var-keyword": true,
      "ordered-imports": true,
      "radix": true,
      "switch-default": true,
      "triple-equals": true,
      "use-isnan": true,
      "indent": [
         true,
         "spaces",
         3
      ],
      "linebreak-style": [
         true,
         "LF"
      ],
      "eofline": true,
      "no-mergeable-namespace": true,
      "object-literal-sort-keys": false,
      "prefer-const": true,
      "max-line-length": false,
      "trailing-comma": [
         true,
         {
            "multiline": "always",
            "singleline": "never"
         }
      ],
      "align": [
         true,
         "arguments",
         "parameters",
         "statements"
      ],
      "array-type": [
         true,
         "generic"
      ],
      "class-name": true,
      "arrow-parens": true,
      "arrow-return-shorthand": true,
      "binary-expression-operand-order": true,
      "comment-format": [
         true,
         "check-space"
      ],
      "interface-name": [
         true,
         "never-prefix"
      ],
      "encoding": true,
      "jsdoc-format": [
         true,
         "check-multiline-start"
      ],
      "one-line": [
         true,
         "check-catch",
         "check-else",
         "check-finally",
         "check-open-brace",
         "check-whitespace"
      ],
      "newline-before-return": true,
      "new-parens": true,
      "no-consecutive-blank-lines": true,
      "no-irregular-whitespace": true,
      "no-trailing-whitespace": true,
      "no-unnecessary-initializer": true,
      "one-variable-per-declaration": true,
      "prefer-function-over-method": true,
      "prefer-switch": true,
      "prefer-template": true,
      "prefer-while": true,
      "quotemark": [
         true,
         "single"
      ],
      "semicolon": [
         true,
         "never"
      ],
      "space-before-function-paren": [
         true,
         {
            "anonymous": "always",
            "named": "never",
            "asyncArrow": "always"
         }
      ],
      "space-within-parens": [
         true,
         0
      ],
      "switch-final-break": [
         true,
         "always"
      ],
      "variable-name": [
         true,
         "check-format",
         "allow-leading-underscore",
         "ban-keywords"
      ],
      "whitespace": [
         true,
         "check-branch",
         "check-decl",
         "check-operator",
         "check-module",
         "check-separator",
         "check-type",
         "check-typecast",
         "check-type-operator",
         "check-preblock"
      ],
      "no-console": [
         true,
         "warning"
      ]
   }
}
